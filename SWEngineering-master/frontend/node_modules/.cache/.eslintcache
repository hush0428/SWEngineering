SET });\n\nconst orderDeliver = useSelector((state) => state.orderDeliver);\n  const {\n    loading: loadingDeliver,\n    error: errorDeliver,\n    success: successDeliver,\n  } = orderDeliver;\n\n  const deliverHandler = () => {\n    dispatch(deliverOrder(order._id));\n  };\n\n   return loading ? (\n     <LoadingBox></LoadingBox>\n   ) : error ? (\n     <MessageBox variant=\"danger\">{error}</MessageBox>\n   ) : (\n     <div>\n       <h1>주문 {order._id}</h1>\n       <div className=\"row top\">\n         <div className=\"col-2\">\n           <ul>\n             <li>\n               <div className=\"card card-body\">\n                 <h2>배송</h2>\n                 <p>\n                   <strong>이름:</strong> {order.shippingAddress.fullName} <br />\n                   <strong>주소: </strong> {order.shippingAddress.address},\n                   {order.shippingAddress.city},{' '}\n                   {order.shippingAddress.postalCode},\n                   {order.shippingAddress.country}\n                 </p>\n                 {order.isDelivered ? (\n                   <MessageBox variant=\"success\">\n                     {order.deliveredAt} 에 배송됨\n                   </MessageBox>\n                 ) : (\n                   <MessageBox variant=\"danger\">배송되지 않음</MessageBox>\n                 )}\n               </div>\n             </li>\n             <li>\n               <div className=\"card card-body\">\n                 <h2>결제</h2>\n                 <p>\n                   <strong>방법:</strong> {order.paymentMethod}\n                 </p>\n                 {order.isPaid ? (\n                   <MessageBox variant=\"success\">\n                     Paid at {order.paidAt}\n                   </MessageBox>\n                 ) : (\n                   <MessageBox variant=\"danger\">미결제</MessageBox>\n                 )}\n               </div>\n             </li>\n             <li>\n               <div className=\"card card-body\">\n                 <h2>주문 상품</h2>\n                 <ul>\n                   {order.orderItems.map((item) => (\n                     <li key={item.product}>\n                       <div className=\"row\">\n                         <div>\n                           <img\n                             src={item.image}\n                             alt={item.name}\n                             className=\"small\"\n                           ></img>\n                         </div>\n                         <div className=\"min-30\">\n                           <Link to={`/product/${item.product}`}>\n                             {item.name}\n                           </Link>\n                         </div>\n\n                         <div>\n                           {item.qty} x ${item.price} = ${item.qty * item.price}\n                         </div>\n                       </div>\n                     </li>\n                   ))}\n                 </ul>\n               </div>\n             </li>\n           </ul>\n         </div>\n         <div className=\"col-1\">\n           <div className=\"card card-body\">\n             <ul>\n               <li>\n                 <h2>주문 요약</h2>\n               </li>\n               <li>\n                 <div className=\"row\">\n                   <div>상품</div>\n                   <div>${order.itemsPrice.toFixed(2)}</div>\n                 </div>\n               </li>\n               <li>\n                 <div className=\"row\">\n                   <div>배송</div>\n                   <div>${order.shippingPrice.toFixed(2)}</div>\n                 </div>\n               </li>\n              \n               <li>\n                 <div className=\"row\">\n                   <div>\n                     <strong> 주문 합계</strong>\n                   </div>\n                   <div>\n                     <strong>${order.totalPrice.toFixed(2)}</strong>\n                   </div>\n                 </div>\n               </li>\n               {userInfo.isAdmin && //order.isPaid && \n               !order.isDelivered && (\n                <li>\n                  {loadingDeliver && <LoadingBox></LoadingBox>}\n                  {errorDeliver && (\n                    <MessageBox variant=\"danger\">{errorDeliver}</MessageBox>\n                  )}\n                  <button\n                    type=\"button\"\n                    className=\"primary block\"\n                    onClick={deliverHandler}\n                  >\n                    배송중으로 변환\n                  </button>\n                </li>\n              )}\n             </ul>\n           </div>\n         </div>\n       </div>\n     </div>\n   );\n }","/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/screens/CartScreen.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/reducers/orderReducers.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/reducers/productReducers.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/reducers/userReducers.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/components/PrivateRoute.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/screens/OrderListScreen.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/reducers/cartReducers.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/components/MessageBox.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/components/Rating.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/actions/orderActions.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/actions/productActions.js",[],"/Users/kimsanghyun/Desktop/GitHub/SWEngineering/SWEngineering-master/frontend/src/components/CheckoutSteps.js",[],"/Users/kims